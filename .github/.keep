  nav {
    float: right;
    margin-right: 70px;
    text-shadow:1px 1px #8f6bd1;
  }
  
  nav ul {
    margin: 10;
    padding: 0;
    list-style: none;
  }
  
  nav li {
    display: inline-block;
    margin-left: 70px;
    padding-top: 10px;
  
    position: relative;
  }
  
  nav a {
    color: #ffffff;
    text-decoration: none;
    text-transform: uppercase;
    font-size: 14px;
  }
  
  nav a:hover  {
    color: #ffffff;
  }
  
  nav a: :before {
    content: '';
    display: block;
    height: 5px;
    background-color: #fff
     
    top: 0%
    width: 0%;
   
  
    transition: all ease-in-out 250ms;  
  }
  
  nav a: hover: :before {
    width: 100%:
  }
  
